#include <stdio.h>
#include <string.h>
int main()
{
    char nome[80], email[50], numero[20];
    printf("Digite o nome: ");
    gets(nome);
    printf("Digite o e-mail: ");
    gets(email);
    printf("Digite o numero de telefone: ");
    gets(numero);

    printf("\nNome = %zu \n",strlen(nome));
    printf("E-mail = %zu \n",strlen(email));
    printf("Telefone = %zu \n",strlen(numero));

    return 0;
}
